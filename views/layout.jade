doctype html
html
	head
		title= title
		link(rel='stylesheet', href='/static/css/bootstrap.min.css')
		link(rel='stylesheet', href='/static/css/bootswatch.min.css')
		link(rel='stylesheet', href='/static/css/style.css')
	body
		.navbar.navbar-default.navbar-fixed-top
			.container
				.navbar-header
					button.navbar-toggle.collapsed(type='button', data-toggle='collapse', data-target='#navbar-main')
						span.icon-bar
						span.icon-bar
						span.icon-bar
					a(href='/').navbar-brand Herf.IO
				#navbar-main.navbar-collapse.collapse
					ul.nav.navbar-nav
						li
							a(href='/bids') BidHistory
						li
							a(href='/humidors/list') HumiTrack
					ul.nav.navbar-nav.pull-right
						if !user
							li
								a(href='/login') Login
						else
							li
								a(href='/logout') Logout
							li#username= user.name
		div#wrapper.container
			block content
	script(src='/static/js/jquery.min.js')
	script(src='/static/js/jquery.flot.min.js')
	script(src='/static/js/jquery.flot.time.min.js')
	script(src='/static/js/jquery.flot.tooltip.min.js')
	script(src='/static/js/jquery.serialize-object.min.js')
	script(src='/static/js/herfio.js')
	block script